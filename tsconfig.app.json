{
  "compilerOptions": {
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["es2017"],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noEmit": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}